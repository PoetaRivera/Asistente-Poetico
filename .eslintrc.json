{"env": {
    "browser": true,
    "es2021": true,
    "jquery": true,
    "node": false
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": ["warn"],
    "no-console": "off",
    "no-undef": "error"
  }
}